{
	"singleQuote": true,
	"jsxDoubleQuote": true,
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^react",
		"^next",
		"react",
		"<THIRD_PARTY_MODULES>",
		"^@",
		".svg",
		"^../(.*)$",
		"^./(.*)$",
		".css"
	]
}
